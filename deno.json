{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "tasks": {
    "deploy:staging": "deployctl deploy --project=oscar-staging --import-map ./import_map.json ./src/index.ts",
    "deploy:prod": "deployctl deploy --project=oscar-prod --import-map ./import_map.json ./src/index.ts"
  },
  "importMap": "./import_map.json",
  "fmt": {
    "options": {
      "lineWidth": 120
    }
  }
}
